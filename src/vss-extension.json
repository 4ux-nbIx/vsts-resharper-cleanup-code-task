{
  "manifestVersion": 1,
  "id": "resharper-cleanup-code",
  "name": "Resharper Cleanup Code",
  "version": "1.0.18",
  "publisher": "RomanNovitsky",
  "public": false,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Run the Resharper Command-Line Tool and automatically cleanup and format code. Configure team-shared coding rules for seamless code style standards enforcement on each commit",
  "categories": [
    "Build and release"
  ],
  "icons": {
    "default": "extension-icon.png"
  },
  "tags": [
    "resharper",
    "code quality",
    "code cleanup",
    "code format"
  ],
  "files": [
    {
      "path": "task"
    }
  ],
  "contributions": [
    {
      "id": "custom-build-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Task"
      }
    }
  ],
  "scopes": [
    "vso.build",
    "vso.build_execute"
  ],
  "links": {
    "learn": {
      "uri": "https://www.jetbrains.com/help/resharper/CleanupCode.html"
    }
  },
  "content": {
    "details": {
      "path": "README.md"
    }
  }
}